=\"mr-sm-2\" placeholder=\"Search\" v-model=\"text\"></b-form-input>\n</b-col>\n<b-col sm=\".5\">\n      <b-button size=\"sm\" class=\"my-2 my-sm-0\" v-on:click=\"search(text)\">Search</b-button>\n</b-col>\n\n      </v-app-bar>\n<!--\n        <v-app-bar app clipped-left color=\"blue\">\n          <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"> = </v-app-bar-nav-icon>\n          <v-toolbar-title>Title</v-toolbar-title>\n          <Header :menu=\"menu\" /> \n        </v-app-bar>\n      \n      <v-navigation-drawer \n      app \n      clipped \n      v-model=\"drawer\"\n      \n      >\n    \n    <button> JSON </button>\n    <b-button  v-on:click=\"test()\" variant=\"primary\"> Change JSON </b-button>\n      </v-navigation-drawer>\n-->\n      <v-navigation-drawer\n        app\n        clipped\n        permanent\n        expand-on-hover\n        :mini-variant.sync=\"mini\"\n      >\n\n      <v-list>\n\n      <v-list-item link>\n            <v-list-item-content>\n              <v-list-item-title class=\"text-h6\">\n                Current File\n              </v-list-item-title>\n              <v-list-item-subtitle>sandra_a88@gmail.com</v-list-item-subtitle>\n            </v-list-item-content>\n          </v-list-item>\n      </v-list>\n\n      <v-divider></v-divider>\n\n        <v-list\n          dense\n          rounded\n        >\n          <v-list-item-group\n          v-model=\"group\"\n          active-class=\"deep-purple--text text--accent-4\"\n          link\n        >\n          <v-list-item @click=\"upload(); drawer = false\">\n            <v-list-item-icon>\n              <v-icon>mdi-cloud-upload</v-icon>\n            </v-list-item-icon>\n            <v-list-item-title>Upload Graph</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"drawer = false\">\n            <v-list-item-icon>\n              <v-icon>mdi-cloud-download</v-icon>\n            </v-list-item-icon>\n            <v-list-item-title>Download Graph</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n            <v-list-item-icon>\n              <v-icon>mdi-code-json</v-icon>\n            </v-list-item-icon>\n          \n\n          <v-dialog\n      v-model=\"dialog\"\n      width=\"550\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-list-item-title \n        v-bind=\"attrs\"\n          v-on=\"on\"\n        >Generate Data</v-list-item-title>\n      </template>\n\n      <v-card>\n        <v-card-title class=\"text-h5 grey lighten-2\">\n          Generate Data From Graph\n        </v-card-title>\n\n        <v-card-text>\n          <b-form-input id=\"range-2\" v-model=\"value\"  type=\"range\" min=\"50\" max=\"500\" step=\"50\" debounce=\"500\"></b-form-input>\n          <div class=\"mt-2\">Amount: {{ value }}</div>\n        </v-card-text>\n\n        <v-divider></v-divider>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"primary\"\n            text\n            @click=\"drawer = false; generateData(value)\"\n          >\n            Generate\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n\n    </v-list-item>\n\n        </v-list-item-group>\n\n        <v-divider></v-divider>\n\n<v-list-item>\n            <v-list-item-content>\n              <v-list-item-title class=\"text-h6\">\n                Selected Node\n              </v-list-item-title>\n              <p></p>\n              <v-list-item-subtitle>Name:</v-list-item-subtitle>\n\n              \n\n              </v-list-item-content>\n          </v-list-item>\n\n          <v-list-item @click=\"drawer = false\">\n            <v-list-item-icon>\n              <v-icon>mdi-pencil-outline</v-icon>\n            </v-list-item-icon>\n            <v-list-item-title>\n            <b-form-input\n                id=\"inline-form-input-name\"\n                v-model=\"selected.sel[0].name\"\n                :placeholder=\"selected.sel[0].name\"\n              ></b-form-input>\n              </v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"drawer = false; deleteNode(selected.sel[0])\">\n            <v-list-item-icon>\n              <v-icon>mdi-delete</v-icon>\n            </v-list-item-icon>\n            <v-list-item-title>Delete Node</v-list-item-title>\n          </v-list-item>\n\n        </v-list>\n      </v-navigation-drawer>\n  \n  <v-main app>\n  <v-container app absolute>\n  <CytoscapeGraph :test=\"test\"/>\n\n  <label for=\"input-medium\">Selected Node</label>\n\n  <b-row class=\"my-1\">\n    <b-col sm=\"1\">\n      <label class=\"sr-only\" for=\"input-medium\">Name:</label>\n    </b-col>\n\n    <b-col sm=\"2\">\n    <b-form-input\n      id=\"inline-form-input-name\"\n      v-model=\"selected.sel[0].name\"\n      :placeholder=\"selected.sel[0].name\"\n    ></b-form-input>\n    </b-col>\n  \n    <b-col sm=\"2\">\n      <b-button variant=\"danger\" v-on:click=\"deleteNode(selected.sel[0])\">Delete Node</b-button>\n    </b-col>\n</b-row>\n\n  </v-container>\n  </v-main>\n\n    </v-app>\n  </div>\n</template>\n\n<script>\n//import Header from \"./components/Header\";\nimport CytoscapeGraph from \"./components/CytoscapeGraph\";\n//import DataList from './components/DataList.vue';\n//import listTest from './data/start'\n//import Start from './components/Start.vue';\n//import JSON from './components/JSON.vue'\n//console.log(listTest)\nimport { jsonData } from \"./data/data\"\nimport { selectedNode } from \"./data/selected\"\nvar test\nconst axios = require('axios').default\n//var ex = {\"nodes\": [{\"data\": {\"id\": \"x1\", \"name\": \"x1\"}}, {\"data\": {\"id\": \"x2\", \"name\": \"x2\"}}, {\"data\": {\"id\": \"x3\", \"name\": \"x3\"}}, {\"data\": {\"id\": \"x4\", \"name\": \"x4\"}}], \"edges\": [{\"data\": {\"id\": \"edge0\", \"source\": \"x2\", \"target\": \"x1\"}}, {\"data\": {\"id\": \"edge1\", \"source\": \"x3\", \"target\": \"x1\"}}, {\"data\": {\"id\": \"edge2\", \"source\": \"x4\", \"target\": \"x1\"}}, {\"data\": {\"id\": \"edge3\", \"source\": \"x4\", \"target\": \"x2\"}}, {\"data\": {\"id\": \"edge4\", \"source\": \"x4\", \"target\": \"x3\"}}]}\n\nexport default {\n  name: \"App\",\n  components: {\n    CytoscapeGraph\n  },\n  \n  data() {\n    return {\n      test,\n      text: '',\n      dialog: false,\n      info: null,\n      showGraph: false,\n      drawer: false,\n      mini: true,\n      sheet: false,\n      value: selectedNode.state.amount,\n      items: [\n        { title: 'Upload Graph', icon: 'mdi-view-dashboard' },\n        { title: 'Save Current Graph', icon: 'mdi-forum' },\n        { title: 'Generate Data', icon: 'mdi-forum' },\n      ],\n      nodeS: [\n        selectedNode.state.node,\n      ],\n      menu: [\n        { icon: \"home\", title: \"Home\" },\n        { icon: \"info\", title: \"Cytoscape\" },\n      ], \n      popupDefaultConfig: {\n        title: 'ciao-vue-popup',\n        apply: {\n          extraClass: 'btn btn-sm btn-info',\n        },\n        prompt: {\n          inputCustomClass: 'form-control form-control-sm',\n        },\n      }\n    };\n  }, \n\n  computed: {\n    selected() {\n      return {\n        sel: [\n          selectedNode.state.node\n        ], \n        value: selectedNode.state.amount\n      }\n    }\n  },\n  methods: {\n    graph() {\n      console.log(\"GRAPH\")\n      this.showGraph = !this.showGraph\n    },\n    upload() {\n      getTheFile()\n    }, \n    search(name) {\n      console.log(name)\n      //selectedNode.commit('changeNode', name );\n      jsonData.commit('searchNode', name );\n    },\n    generateData(value){\n      console.log(value)\n      console.log(\"-------------------------------------\")\n      console.log(jsonData.getters.getNode)\n      \n      axios\n      .post('http://127.0.0.1:8000/getData?no_samples=' + value, jsonData.getters.getNode)\n      .then(response => {\n        console.log(\"-----------------*******--------------------\")\n        console.log(response)\n\n        saveJSON(response.data, 'genData-' + value + '.json')\n      })\n      .catch(error => {\n        console.log(error)\n      })\n      \n    },\n    deleteNode(node) {\n      //selectedNode.commit('changeNode', node );\n      console.log(\"DELETE\")\n      console.log(node.id)\n      //this.nodeS = null\n      //ur.do(\"remove\", selectedNode.state.node);\n      jsonData.commit('removeNode', node.id)\n    },\n  }\n};\n\nconst pickerOpts = {\n  types: [\n    {\n      description: 'JSON',\n      accept: {\n        'JSON/*': ['.json']\n      }\n    },\n  ],\n  excludeAcceptAllOption: true,\n  multiple: false\n};\n\nasync function getTheFile() {\n  // open file picker\n  var fileHandle = null;\n  [fileHandle] = await window.showOpenFilePicker(pickerOpts);\n  console.log(fileHandle);\n\n  // get file contents\n  const fileData = await fileHandle.getFile();\n  console.log(\"FILE DATA: \" + fileData.name);\n\n  jsonData.commit('changeJSON', fileData)\n  //loadFile(\"test_data/\" + fileData.name);\n}\n\nfunction saveJSON(data, filename){\n    if(!data) {\n        console.error('No data')\n        return;\n    }\n    if(!filename) filename = 'console.json'\n\n    if(typeof data === \"object\"){\n        data = JSON.stringify(data, undefined, 4)\n    }\n    var blob = new Blob([data], {type: 'text/json'}),\n        e    = document.createEvent('MouseEvents'),\n        a    = document.createElement('a')\n\n    a.download = filename\n    a.href = window.URL.createObjectURL(blob)\n    a.dataset.downloadurl =  ['text/json', a.download, a.href].join(':')\n    e.initMouseEvent('click', true, false, window, 0, 0, 0, 0, 0, false, false, false, false, 0, null)\n    a.dispatchEvent(e)\n}\n\n/*\n<v-btn @click=\"graph()\"> SHOW </v-btn>\n        <div v-if=\"showGraph\">\n*/\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n</style>\n\n<style>\n  @import './assets/styles/cytoscape.js-navigator.css';\n</style>\n\n<style>\n  @import './assets/styles/cytoscape.js-panzoom.css';\n</style>\n\n<style src=\"ciao-vue-popup/dist/ciao-vue-popup.css\"></style>\n\n\n// <div class=\"json\">\n//             <v-btn block v-on:click=\"load()\"> Load File </v-btn>\n//           </div>\n\n"]}]}